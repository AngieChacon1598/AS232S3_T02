    <queryString language="SQL">
        <![CDATA[
        SELECT
            u.id_user,
            u.username,
            u.names,
            u.lastnames,
            u.phone,
            u.email,
            u.number_identification,
            u.type_document,
            t.name AS user_type,
            u.status,
            u.email_verified
        FROM
            users.user_restaurant u
        JOIN
            users.type_user t ON u.id_type_user = t.id_type_user
        WHERE
            u.status = 1
        ]]>
    </queryString>

